

<%= image_tag "logo", size: "150x150" %>

<h1>Rooms</h1>

<% @rooms.each do |room| %>
	<p>
		<%= link_to room do %>
			<button><%= room.name %></button>
		<% end %>
	</p>
<% end %>

<% if current_admin || current_teacher %>
	<%= link_to new_room_path do %>
		<button class="lighter_button">New Room</button>
	<% end %>
<% end %>
